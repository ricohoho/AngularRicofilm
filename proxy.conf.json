{
  "/films/list/*": {
    "target": "http://localhost:3000",
    "secure": false,
    "pathRewrite": {
        "^/films/list/": "/films/list/"
    },
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/request/add*": {
    "target": "http://localhost:3000",
    "secure": false,
    "pathRewrite": {
      "^/request/add": "/request/add"
    },
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/api/*": {
    "target": "http://localhost:3000",
    "secure": false,
    "pathRewrite": {
      "^/api": "/api"
    },
    "changeOrigin": true,
    "logLevel": "debug"
  },
  "/image/*": {
    "target": "http://localhost:3000",
    "secure": false,
    "pathRewrite": {
      "^/api": "/image"
    },
    "changeOrigin": true,
    "logLevel": "debug"
  }


}
